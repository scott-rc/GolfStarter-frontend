div
  h2 Change Password
  .ui.form
    .ui.field
      .field
        label Password
        input(id="currentPassword", type="password" v-model="model.password")
    .ui.field
      .field
        label New Password
        input(id="newPassword", type="password", v-model="model.newPassword")
    .ui.primary.submit.button(@click="changePassword") Submit